{
    "type": "vless",
    "tag": "vless-in",
    "listen": "::",
    "listen_port": 5353,
    "users": [
        {
            "uuid": "bf000d23-0752-40b4-affe-68f7707a9661"
        }
    ],
    "tls": {
        "enabled": true,
        "server_name": "bing.com",
        "key_path": "/path/to/your/key.pem",
        "certificate_path": "/path/to/your/cert.pem"
    },
    "multiplex": {
        "enabled": true
    },
    "transport": {
        "type": "ws",
        "path": "/",
        "headers": {
            "host": "bing.com"
        },
        "max_early_data": 2048,
        "early_data_header_name": "Sec-WebSocket-Protocol"
    }
}